{
  "name": "evotodi/password-meter-bundle",
  "description": "Password meter for symfony 6 that complements https://github.com/HamedFathi/PasswordMeter and passwordmeter.com",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "evotodi",
      "email": "evotodi@gmail.com"
    }
  ],
  "require": {
    "php": "^8",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/config": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0",
    "symfony/phpunit-bridge": "^6.0|^7.0",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Evotodi\\PasswordMeterBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Evotodi\\PasswordMeterBundle\\Tests\\": "tests/"
    }
  }
}
